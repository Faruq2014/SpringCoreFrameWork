<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="Faruq"
		class="com.mollaConstructorDI.Employee">
		<constructor-arg name="eName" value="Faruq" type="java.lang.String"/>
		<constructor-arg name="salary" value="1000" type="Double"/>
	</bean>

	<bean id="Khaled"
		class="com.mollaConstructorDI.Employee">
		<constructor-arg name="eName" value="Khaled" />
		<constructor-arg name="salary" value="2000" type="Double"/>
	</bean>

</beans>